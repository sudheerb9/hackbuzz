    <section id="register" class="s-portfolio">
        
        <div class="row">
            <div class="column large-12">
                <div class="section-title" data-num="03" data-aos="fade-up">
                    <h3 class="h6">Register </h3>
                </div>
            </div>
           
            <form style="width: 100%;">
                <div class="row">
                    <div class="form-group column large-6">
                        <label>Full Name</label>
                        <input type="text" class="form-control" name="fullname" placeholder="Enter your full name" required>
                    </div>
                    <div class="form-group column large-6">
                        <label>Email address</label>
                        <input type="email" class="form-control" name="email" placeholder="Enter email" required>
                    </div>
                </div>
                <div class="row">    
                    <div class="form-group column large-6 column-small-12">
                    <label>Mobile Number (preferably whatsapp)</label>
                    <input type="number" class="form-control" name="phone" placeholder="Enter your mobile number" required>
                    </div>
                    <div class="form-group column large-6 column-small-12">
                        <label>Year and Branch of Study</label>
                        <input type="text" class="form-control" name="year" placeholder="Eg: 3rd Year Mechanical" required>
                    </div>
                </div>
                <div class="row">    
                    <div class="form-group column large-6 column-small-12">
                        <label>Institute</label>
                        <input type="text" class="form-control" name="college" placeholder="Eg: IIT Bhubaneswar" required>
                    </div>
                    <div class="form-group column large-6 column-small-12">
                        <label>State</label>
                        <input type="text" class="form-control" name="state" placeholder="Eg: Odisha" required>
                    </div>
                </div>

                <div class="row">    
                    <div class="form-group column large-6 column-small-12">
                        <label>Reference ID of CA (If any)</label>
                        <input type="text" class="form-control" name="refca" placeholder="Eg: W21CA0001">
                    </div>
                    
                </div>
                
                <button type="submit" id="submit" class="btn btn-dark">Submit</button>
              </form>
              <script>
                  $("form").submit(function(e) {
                    e.preventDefault(); 
                    var form = $(this);
                    $.ajax({
                        type: "POST",
                        url: '/submit',
                        data: form.serialize(), 
                        success: function(result)
                        {
                            if(result === 'Already registered using this email'){
                                new swal({
                                    title: 'Error',
                                    text: result,
                                    icon: 'error'
                                });
                            }
                            else{
                                new swal({
                                    title: 'Success',
                                    text: result,
                                    icon: 'success'
                                });
                            }
                        }
                        });
                    });
              </script>
        </div>
        

